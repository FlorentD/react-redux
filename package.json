{
  "name": "react-redux",
  "description": "what i need to start a react-redux app",
  "version": "1.0.0",
  "private": false,
  "main": "index.js",
  "repository": "git@github.com:FlorentD/react-redux.git",
  "author": "florentduveau <florent.duveau@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "precommit": "lint-staged",
    "predist": "rm -rf app && rm -rf dist && npx babel server --out-dir app/server && npx babel public --out-dir app/public && mkdir dist",
    "dist": "webpack --config webpack-prod.config.js --progress --colors && npm run copy",
    "postdist": "rm -fr dist",
    "prettier": "prettier --single-quote --trailing-comma es5 --write",
    "copy": "cp dist/*.js app/server/static/ && cp dist/*.json app/server/static/",
    "test": "jest --config tests/jest.config.json",
    "dev": "clear && webpack-dev-server",
    "deploy": "npm run test && npm run dist && git push heroku master",
    "start": "node ./app/server/index.js",
    "ssr": "nodemon --exec babel-node server/index.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier",
      "git add"
    ]
  },
  "dependencies": {
    "express": "4.17.1",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.14",
    "mongodb": "3.2.7",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-loadable": "^5.5.0",
    "react-redux": "7.1.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-react-hook": "3.3.2",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "restify": "8.3.3",
    "styled-components": "4.3.2",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/node": "7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "7.5.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "@babel/plugin-transform-runtime": "7.5.0",
    "@babel/preset-env": "7.5.4",
    "@babel/runtime": "7.5.4",
    "acorn": "6.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.6",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-styled-components": "1.10.6",
    "babel-regenerator-runtime": "6.5.0",
    "chai": "4.2.0",
    "chai-enzyme": "beta",
    "compression": "1.7.4",
    "css-loader": "3.0.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "lint-staged": "9.2.0",
    "prettier": "1.18.2",
    "react-hot-loader": "4.12.6",
    "react-test-renderer": "16.8.6",
    "sinon": "7.3.2",
    "sinon-chai": "3.3.0",
    "sinon-test": "2.4.0",
    "style-loader": "0.23.1",
    "uglifycss": "0.0.29",
    "url-loader": "2.0.1",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2"
  }
}
