{
  "name": "react-redux",
  "description": "what i need to start a react-redux app",
  "version": "1.0.0",
  "private": false,
  "main": "index.js",
  "repository": "git@github.com:FlorentD/react-redux.git",
  "author": "florentduveau <florent.duveau@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "8.x || 9.x"
  },
  "scripts": {
    "precommit": "lint-staged",
    "predist": "rm -fr dist && mkdir dist && mkdir dist/js",
    "prettier": "prettier --single-quote --trailing-comma es5 --write",
    "dist": "webpack --config webpack-prod.config.js --progress --colors && npm run dist:html",
    "dist:html": "htmlprocessor public/index.html -o dist/index.html",
    "test": "jest --config tests/jest.config.json",
    "dev": "clear && webpack-dev-server",
    "deploy": "npm run test && npm run dist && git push heroku master",
    "start": "node server-rest.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier",
      "git add"
    ]
  },
  "dependencies": {
    "express": "4.16.4",
    "lodash": "4.17.11",
    "mongodb": "3.1.10",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "recompose": "0.30.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "restify": "7.3.0",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-transform-react-jsx": "7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.4",
    "babel-regenerator-runtime": "6.5.0",
    "chai": "4.2.0",
    "chai-enzyme": "beta",
    "compression": "1.7.3",
    "css-loader": "2.0.2",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "es6-promise": "4.2.5",
    "htmlprocessor": "0.2.6",
    "husky": "1.2.1",
    "isomorphic-fetch": "2.2.1",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "prettier": "1.15.3",
    "react-hot-loader": "4.6.3",
    "react-test-renderer": "16.7.0",
    "sinon": "7.2.2",
    "sinon-chai": "3.3.0",
    "sinon-test": "2.4.0",
    "style-loader": "0.23.1",
    "uglifycss": "0.0.29",
    "url-loader": "1.1.2",
    "webpack": "4.28.2",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.14"
  }
}
