{
  "name": "react-redux",
  "description": "what i need to start a react-redux app",
  "version": "1.0.0",
  "private": false,
  "main": "index.js",
  "repository": "git@github.com:FlorentD/react-redux.git",
  "author": "florentduveau <florent.duveau@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "precommit": "lint-staged",
    "predist": "rm -rf app && rm -rf dist && npx babel server --out-dir app/server && npx babel public --out-dir app/public && mkdir dist && mkdir app/server/image",
    "dist": "webpack --config webpack-prod.config.js --progress --colors && npm run copy",
    "postdist": "rm -fr dist",
    "prettier": "prettier --single-quote --trailing-comma es5 --write",
    "copy": "cp dist/*.js app/server/static/ && cp dist/*.json app/server/static/ && cp server/static/manifest.json app/server/static && cp server/image/*.png app/server/image/ && cp server/image/*.jpg app/server/image/ && cp server/static/*.html app/server/static && cp server/static/*.css app/server/static && cp server/favicon.ico app/server",
    "build": "rm -rf dist && webpack --config webpack-prod.config.js && cp dist/*.* server/static",
    "test": "jest --config tests/jest.config.json",
    "dev": "clear && webpack-dev-server",
    "deploy": "npm run test && npm run dist && git push heroku master",
    "start": "node ./app/server/index.js",
    "ssr": "nodemon --exec babel-node server/index.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier",
      "git add"
    ]
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "dexie": "2.0.4",
    "express": "4.17.1",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.15",
    "mongodb": "3.3.3",
    "prop-types": "15.7.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-loadable": "5.5.0",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "restify": "8.4.0",
    "semantic-ui-react": "0.88.1",
    "styled-components": "4.4.1",
    "web-push": "3.4.1",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/node": "7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-react-jsx": "7.7.0",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.7.1",
    "@babel/runtime": "7.7.2",
    "acorn": "7.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.6",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-styled-components": "1.10.6",
    "babel-regenerator-runtime": "6.5.0",
    "chai": "4.2.0",
    "chai-enzyme": "beta",
    "css-loader": "3.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "react-hot-loader": "4.12.16",
    "react-test-renderer": "16.11.0",
    "sinon": "7.5.0",
    "sinon-chai": "3.3.0",
    "sinon-test": "2.4.0",
    "style-loader": "1.0.0",
    "uglifycss": "0.0.29",
    "url-loader": "2.2.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0"
  }
}
