{
  "name": "react-redux",
  "description": "what i need to start a react-redux app",
  "version": "1.0.0",
  "private": false,
  "main": "index.js",
  "repository": "git@github.com:FlorentD/react-redux.git",
  "author": "florentduveau <florent.duveau@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "precommit": "lint-staged",
    "predist": "rm -rf app && rm -rf dist && npx babel server --out-dir app/server && npx babel public --out-dir app/public && mkdir dist && mkdir app/server/image",
    "dist": "webpack --config webpack-prod.config.js --progress --color && npm run copy",
    "postdist": "rm -fr dist",
    "prettier": "prettier --single-quote --trailing-comma es5 --write",
    "copy": "cp -R dist/. app/server/static/ && cp server/image/*.png app/server/image/ && cp server/image/*.jpg app/server/image/ && cp server/favicon.ico app/server && cp server/static/manifest.json app/server && cp server/robots.txt app/server",
    "build": "rm -rf dist && webpack --config webpack-prod.config.js && cp dist/*.* server/static",
    "test": "jest --config tests/jest.config.json",
    "dev": "clear && webpack serve",
    "deploy": "npm run test && npm run dist && git add . && git commit -m 'deploy to heroku' && git push heroku master",
    "start": "node ./app/server/index.js",
    "ssr": "ASSETS_STRATEGY='production' nodemon --exec babel-node server/index.js",
    "cypress:open": "cypress open",
    "cy:run": "cypress run --spec ./cypress/integration/sanity/welcome.spec.js --record --key 42791497-6ed9-4f3d-8661-87f9590ced14",
    "test:ci": "start-server-and-test start http://localhost:8080 cy:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@apollo/client": "3.3.12",
    "@babel/node": "7.13.12",
    "@loadable/component": "5.14.1",
    "apollo-server-express": "2.21.2",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "dexie": "3.0.3",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "lodash": "4.17.21",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "styled-components": "5.2.1",
    "web-push": "3.4.4",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.13.10",
    "@babel/preset-env": "7.13.12",
    "@babel/preset-react": "7.12.13",
    "@babel/runtime": "7.13.10",
    "@pmmmwh/react-refresh-webpack-plugin": "next",
    "@wojtekmaj/enzyme-adapter-react-17": "0.5.0",
    "acorn": "8.1.0",
    "autoprefixer": "10.2.5",
    "babel-loader": "8.2.2",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-styled-components": "1.12.0",
    "chai": "4.3.4",
    "chai-enzyme": "beta",
    "css-loader": "5.1.3",
    "cssnano": "4.1.10",
    "cypress": "6.8.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "file-loader": "6.2.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.1",
    "husky": "5.2.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "mini-css-extract-plugin": "1.3.9",
    "postcss": "8.2.8",
    "postcss-import": "14.0.0",
    "postcss-loader": "5.2.0",
    "postcss-url": "10.1.3",
    "prettier": "2.2.1",
    "react-hot-loader": "4.13.0",
    "react-refresh": "0.9.0",
    "react-test-renderer": "17.0.2",
    "sinon": "10.0.0",
    "sinon-chai": "3.6.0",
    "sinon-test": "3.0.0",
    "start-server-and-test": "1.12.1",
    "style-loader": "2.0.0",
    "tailwindcss": "2.0.4",
    "uglifycss": "0.0.29",
    "url-loader": "4.1.1",
    "webpack": "5.27.2",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "next",
    "webpack-manifest-plugin": "3.1.0"
  }
}
