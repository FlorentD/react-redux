{
  "name": "react-redux",
  "description": "what i need to start a react-redux app",
  "version": "1.0.0",
  "private": false,
  "main": "index.js",
  "repository": "git@github.com:FlorentD/react-redux.git",
  "author": "florentduveau <florent.duveau@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "precommit": "lint-staged",
    "predist": "rm -rf app && rm -rf dist && npx babel server --out-dir app/server && npx babel public --out-dir app/public && mkdir dist && mkdir app/server/image",
    "dist": "webpack --config webpack-prod.config.js --progress --colors && npm run copy",
    "postdist": "rm -fr dist",
    "prettier": "prettier --single-quote --trailing-comma es5 --write",
    "copy": "cp dist/*.js app/server/static/ && cp dist/*.json app/server/static/ && cp server/static/manifest.json app/server/static && cp server/image/*.png app/server/image/ && cp server/image/*.jpg app/server/image/ && cp server/static/*.html app/server/static && cp server/static/*.css app/server/static && cp server/favicon.ico app/server",
    "build": "rm -rf dist && webpack --config webpack-prod.config.js && cp dist/*.* server/static",
    "test": "jest --config tests/jest.config.json",
    "dev": "clear && webpack-dev-server",
    "deploy": "npm run test && npm run dist && git push heroku master",
    "start": "node ./app/server/index.js",
    "ssr": "nodemon --exec babel-node server/index.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier",
      "git add"
    ]
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "dexie": "2.0.4",
    "express": "4.17.1",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.15",
    "mongodb": "3.5.6",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "restify": "8.5.1",
    "semantic-ui-react": "0.88.2",
    "styled-components": "5.1.0",
    "web-push": "3.4.3",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "7.9.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-react-jsx": "7.9.4",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/runtime": "7.9.2",
    "acorn": "7.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.1.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-styled-components": "1.10.7",
    "babel-regenerator-runtime": "6.5.0",
    "chai": "4.2.0",
    "chai-enzyme": "beta",
    "css-loader": "3.5.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.2.0",
    "husky": "4.2.5",
    "jest": "25.4.0",
    "lint-staged": "10.1.7",
    "prettier": "2.0.5",
    "react-hot-loader": "4.12.20",
    "react-test-renderer": "16.13.1",
    "sinon": "9.0.2",
    "sinon-chai": "3.5.0",
    "sinon-test": "3.0.0",
    "style-loader": "1.2.0",
    "uglifycss": "0.0.29",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
}
