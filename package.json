{
  "name": "react-redux",
  "description": "what i need to start a react-redux app",
  "version": "1.0.0",
  "private": false,
  "main": "index.js",
  "repository": "git@github.com:FlorentD/react-redux.git",
  "author": "florentduveau <florent.duveau@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "8.x || 9.x"
  },
  "scripts": {
    "precommit": "lint-staged",
    "predist": "rm -fr dist && mkdir dist && mkdir dist/js",
    "prettier": "prettier --single-quote --trailing-comma es5 --write",
    "dist": "webpack --config webpack-prod.config.js --progress --colors && npm run dist:html && npm run dist:min",
    "dist:html": "htmlprocessor public/index.html -o dist/index.html",
    "dist:min": "minify dist/js/ --out-dir dist/js/",
    "test": "jest --config tests/jest.config.json",
    "dev": "clear && webpack-dev-server",
    "deploy": "npm run test && npm run dist && git push heroku master",
    "start": "node server-rest.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier",
      "git add"
    ]
  },
  "dependencies": {
    "express": "4.16.3",
    "lodash": "4.17.10",
    "mongodb": "3.1.1",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "recompose": "0.27.1",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "reselect": "3.0.1",
    "restify": "7.2.1",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-minify": "0.4.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-minify": "0.4.3",
    "babel-regenerator-runtime": "6.5.0",
    "chai": "4.1.2",
    "chai-enzyme": "beta",
    "compression": "1.7.3",
    "css-loader": "1.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "es6-promise": "4.2.4",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "htmlprocessor": "0.2.6",
    "husky": "0.14.3",
    "isomorphic-fetch": "2.2.1",
    "jest": "23.4.2",
    "lint-staged": "7.2.0",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "12.0.0",
    "postcss-loader": "2.1.6",
    "prettier": "1.14.0",
    "react-hot-loader": "4.3.4",
    "react-test-renderer": "16.4.2",
    "sinon": "6.1.4",
    "sinon-chai": "3.2.0",
    "sinon-test": "2.2.1",
    "style-loader": "0.21.0",
    "uglifycss": "0.0.29",
    "url-loader": "1.0.1",
    "webpack": "4.16.4",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5"
  }
}
